<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>urc_nav2_plugins</name>
  <version>0.0.0</version>
  <description>Package containing a variety of Nav2 plugins.</description>
  <maintainer email="aidan.stickan@gmail.com">a-stickan</maintainer>
  <license>MIT</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>nav2_costmap_2d</depend>
  <depend>nav2_msgs</depend>
  <depend>nav2_bringup</depend>
  <depend>map_msgs</depend>
  <depend>pluginlib</depend>
  <depend>urc_util</depend>
  <depend>grid_map_ros</depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <costmap_2d plugin="${prefix}/costmap_2d_plugins.xml"/>
    <build_type>ament_cmake</build_type>
  </export>
</package>
